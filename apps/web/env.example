# 环境变量配置示例
# 复制此文件为 .env.local 或 .env.development / .env.production 进行配置

# ==================== 应用基础配置 ====================
# API 基础 URL
VITE_API_BASE_URL=http://localhost:3000

# 请求超时时间（毫秒）
VITE_REQUEST_TIMEOUT=10000

# 是否启用请求日志
VITE_ENABLE_REQUEST_LOG=false

# Token 存储键名
VITE_TOKEN_KEY=token

# 应用标题
VITE_APP_TITLE=Web App

# 应用版本
VITE_APP_VERSION=1.0.0

# ==================== 项目信息配置 ====================
# 项目名称
VITE_PROJECT_NAME=Web App

# 项目描述
VITE_PROJECT_DESCRIPTION=A modern web application

# 项目版本
VITE_PROJECT_VERSION=1.0.0

# 项目作者
VITE_PROJECT_AUTHOR=

# 项目仓库地址
VITE_PROJECT_REPOSITORY=

# 项目主页
VITE_PROJECT_HOMEPAGE=

# ==================== 第三方服务配置 ====================
# 地图服务配置
VITE_MAP_PROVIDER=
VITE_MAP_API_KEY=
VITE_MAP_BASE_URL=

# 支付服务配置
VITE_PAYMENT_PROVIDER=
VITE_PAYMENT_APP_ID=
VITE_PAYMENT_SECRET_KEY=
VITE_PAYMENT_CALLBACK_URL=

# 分析服务配置
VITE_ANALYTICS_PROVIDER=
VITE_ANALYTICS_TRACKING_ID=
VITE_ANALYTICS_ENABLED=false

# 对象存储服务配置
VITE_STORAGE_PROVIDER=
VITE_STORAGE_ACCESS_KEY=
VITE_STORAGE_SECRET_KEY=
VITE_STORAGE_BUCKET=
VITE_STORAGE_REGION=

# 短信服务配置
VITE_SMS_PROVIDER=
VITE_SMS_ACCESS_KEY=
VITE_SMS_SECRET_KEY=
VITE_SMS_SIGN_NAME=

# 邮件服务配置
VITE_EMAIL_PROVIDER=
VITE_EMAIL_API_KEY=
VITE_EMAIL_FROM_EMAIL=
VITE_EMAIL_FROM_NAME=

# 推送服务配置
VITE_PUSH_PROVIDER=
VITE_PUSH_APP_KEY=
VITE_PUSH_SECRET=

# OAuth服务配置
VITE_OAUTH_PROVIDER=
VITE_OAUTH_CLIENT_ID=
VITE_OAUTH_CLIENT_SECRET=
VITE_OAUTH_REDIRECT_URI=

# ==================== 开发环境配置 ====================
# 是否启用 Mock 数据
VITE_ENABLE_MOCK=false
# Mock API 基础 URL
VITE_MOCK_API_BASE_URL=http://localhost:3001

# ==================== 生产环境配置 ====================
# 是否启用错误上报
VITE_ENABLE_ERROR_REPORT=true
# 错误上报地址
VITE_ERROR_REPORT_URL=
